<?xml version='1.0' encoding='utf-8'?>
<widget id="com.powerteam.where2go" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Where2Go</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <icon density="mdpi" src="platforms/android/res/mipmap-mdpi/ic_launcher.png" />
    <icon density="hdpi" src="platforms/android/res/mipmap-hdpi/ic_launcher.png" />
    <icon density="xhdpi" src="platforms/android/res/mipmap-xhdpi/ic_launcher.png" />
    <icon density="xxhdpi" src="platforms/android/res/mipmap-xxhdpi/ic_launcher.png" />
    <icon density="xxxhdpi" src="platforms/android/res/mipmap-xxxhdpi/ic_launcher.png" />
    <preference name="loglevel" value="DEBUG" />
</widget>
